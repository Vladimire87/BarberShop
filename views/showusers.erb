<h2>Users:</h2>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>#id</th>
      <th>Name</th>
      <th>Phone</th>
      <th>Date</th>
      <th>Barber</th>
      <th>Color</th>
    </tr>
  </thead>
  <tbody>
    <% @db.execute 'select*from Users order by id asc' do |row| %>
      <tr>
        <th scope="row"><%= row['id'] %></th>
        <td><%= row['Name'] %></td>
        <td><%= row['Phone'] %></td>
        <td><%= row['DateStamp'] %></td>
        <td><%= row['Barber'] %></td>
        <td><%= row['Color'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
